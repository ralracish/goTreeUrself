<div class="container ">
  <div class="row">
    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4 mb-5">
      <h1>goTreeUrself!</h1>
    </div>
    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
  </div>
  <div class="row">
    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
      <div class="row mb-2">
        <h2>Start your Time Capsule</h2>
      </div>
      
      <form class="create-form">
        <div class="form-group row mb-2">
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <label for="un">User Name:</label>
          </div>
          <div class="col-8 col-sm-8 col-md-8 col-lg-8 col-xl-8">
            <input type="text" id="un" name="name">
          </div>
        </div>
        <div class="form-group row mb-2">
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <label for="pw">Password:</label>
          </div>
          <div class="col-8 col-sm-8 col-md-8 col-lg-8 col-xl-8">
            <input type="text" id="pw" name="password">
          </div>
        </div>
        <div class="form-group row mb-2">
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <label for="em">User email:</label>
          </div>
          <div class="col-8 col-sm-8 col-md-8 col-lg-8 col-xl-8">
            <input type="text" id="em" name="email">
          </div>
        </div>
        <div class="form-group row mb-2">
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <label for="tph">Tree Photo Link:</label>
          </div>
          <div class="col-8 col-sm-8 col-md-8 col-lg-8 col-xl-8">
            <input type="text" id="tph" name="treepic">
          </div>
        </div>
        <div class="row">
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <button type="submit" id="submit">Add User</button>
          </div>
          <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
            <button type="login" id="login">Login User</button>
          </div>
        </div>
      </form>
    </div>
    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
  </div>

</div>
