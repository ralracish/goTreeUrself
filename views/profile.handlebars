            <button id="upload-tree" data-id="{{userInfo.id}}" class="button hollow success">Upload Your Tree
              Pic!</button>
            <div id='content' data-id="{{userInfo.id}}"></div>
          </div>
          <ul>
            {{#each treeInfo}}
