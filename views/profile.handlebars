<div class="hero-full-screen-pattern">
  <div class="top-content-section">
    <!-- <div class="top-content-section translucent-form-overlay"> -->
    {{> nav/nav-block user=false register=true}}

    <div class="holy-grail-grid">

      <div class="holy-grail-left translucent-form-overlay">
        <div class="imgwidth" style="margin: 0 auto;">
          <h4>{{userInfo.firstName}} {{userInfo.lastName}}</h4>
          {{#if userInfo.profile_image_link}}
          <img class="thumbnail"
            src=https://cdn.filestackcontent.com/cache=false/output=secure:true/security=policy:eyJleHBpcnkiOjI1MjQ2MjYwMDAsImNhbGwiOlsicGljayIsInJlYWQiLCJzdGF0Iiwid3JpdGUiLCJ3cml0ZVVybCIsInN0b3JlIiwiY29udmVydCIsInJlbW92ZSIsImV4aWYiLCJydW5Xb3JrZmxvdyJdfQ==,signature:4b764be43ffa32d14c20d96fe343a777d3e241b8dde4bd78d8820dbb696a6eac/{{userInfo.profile_image_link}}>
            {{else}} <img class="thumbnail" src="https://placehold.it/300x250">
          {{/if}}
          <div class="myinfo" id="profile-info">
            <button id="profile-button" data-id="{{userInfo.id}}" class="button hollow success expanded">Upload
              Pic</button>
            <button type="button" id="add-user" class="button hollow success expanded search-button">Update
              Profile</button>
            <p>I am an environmental enthusiast and peach tree lover!</p>
            <p>{{userInfo.city}}, {{userInfo.state}}</p>
            <p>{{userInfo.country}}</p>
          </div>
        </div>
      </div>

